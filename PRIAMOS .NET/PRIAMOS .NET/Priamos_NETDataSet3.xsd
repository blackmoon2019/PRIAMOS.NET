                <SelectCommand>
                  <DbCommand CommandType="Text" ModifiedByUser="true">
                    <CommandText>SELECT APT.ID, APT.ttl, APT.nam, 
CONCAT(cctOwner.email + ';',cctOwner.email2 +';',cctOwner.email3) AS cctOwnerEmail,
CONCAT(cctTenant.email +';',cctTenant.email2 +';',cctTenant.email3) AS cctTenantEmail,
CONCAT(cctRepresentative.email + ';',cctRepresentative.email2 +';',cctRepresentative.email3  ) AS cctRepresentativeEmail,
           APT.sendEmailOwner, APT.sendEmailTenant, APT.sendEmailRepresentative, APT.OwnerID, APT.TenantID, APT.RepresentativeID, APT.bal_adm
FROM   APT LEFT OUTER JOIN
           CCT AS cctOwner ON cctOwner.ID = APT.OwnerID LEFT OUTER JOIN
           CCT AS cctTenant ON cctTenant.ID = APT.TenantID LEFT OUTER JOIN
           CCT AS cctRepresentative ON cctRepresentative.ID = APT.RepresentativeID
WHERE (APT.sendEmailOwner = 1) AND (APT.bdgID = @bdgID) OR
           (APT.bdgID = @bdgID) AND (APT.sendEmailTenant = 1) OR
           (APT.bdgID = @bdgID) AND (APT.sendEmailRepresentative = 1)
ORDER BY APT.ord</CommandText>
